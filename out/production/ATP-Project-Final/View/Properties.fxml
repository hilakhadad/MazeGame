<GridPane prefHeight="184.0" prefWidth="339.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.PropertiesController">
  <columnConstraints>
      <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="234.0" minWidth="10.0" prefWidth="38.0" />
    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="316.0" minWidth="10.0" prefWidth="193.0" />
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="127.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints maxHeight="55.0" minHeight="10.0" prefHeight="41.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="58.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="59.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="58.0" minHeight="10.0" prefHeight="46.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="28.0" minHeight="8.0" prefHeight="8.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label layoutY="1.0" text="Maze Solver:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label text="Level:" GridPane.columnIndex="1" />
