      <Button fx:id="btn_cancelProperties" mnemonicParsing="false" onAction="#exitProperties" prefHeight="31.0" prefWidth="76.0" text="Cancel" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <ChoiceBox fx:id="solveBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <ChoiceBox fx:id="levelBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
      </ChoiceBox>
   </children>
</GridPane>
